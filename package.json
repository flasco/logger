{
  "name": "@flasco/logger",
  "version": "0.0.3",
  "main": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "test": "jest --no-cache --forceExit --runInBand --detectOpenHandles"
  },
  "devDependencies": {
    "@types/node": "^12.0.10",
    "@typescript-eslint/eslint-plugin": "^1.11.0",
    "@typescript-eslint/parser": "^1.11.0",
    "eslint": "^6.0.1",
    "husky": "^2.7.0",
    "jest": "^24.8.0",
    "lint-staged": "^8.2.1",
    "typescript": "^3.5.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "*.{js,ts}": ["eslint", "git add"]
    },
    "ignore": ["**/dist/**/*", ".eslint*.*"]
  },
  "jest": {
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$",
      "<rootDir>/node_modules/"
    ]
  },
  "dependencies": {
    "colors-cli": "^1.0.23",
    "date-fns": "^1.30.1",
    "tslib": "^1.10.0"
  }
}
